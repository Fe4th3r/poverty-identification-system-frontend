{\r\n              alert(response.data.message);\r\n            }\r\n          } catch (error) {\r\n            console.error('添加特殊群体信息失败:', error);\r\n            alert('添加失败');\r\n          }\r\n        }\r\n  \r\n      },\r\n      async deleteSpecialGroup(id) {\r\n        if (confirm('确定要删除该特殊群体信息吗？')) {\r\n          try {\r\n            await axios.delete(`/special-group/${id}`);\r\n            this.fetchSpecialGroups();\r\n          } catch (error) {\r\n            console.error('删除特殊群体信息失败:', error);\r\n          }\r\n        }\r\n      },\r\n      resetForm() {\r\n        this.showForm = false;\r\n        this.currentSpecialGroup = null;\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 可以根据需要添加样式 */\r\n  </style>"],"mappings":"AAiCE,OAAOA,gBAAe,MAAO,wBAAwB;AACrD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,KAAK;MACfC,mBAAmB,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,kBAAkB,IAAI,CAACZ,SAAS,EAAE,CAAC;QACpE,IAAI,CAACK,aAAY,GAAIM,QAAQ,CAACP,IAAI;MACpC,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,aAAa,EAAEA,KAAK,CAAC;MACrC;IACF,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACR,mBAAkB,GAAI,IAAI;MAC/B,IAAI,CAACD,QAAO,GAAI,IAAI;IACtB,CAAC;IACDU,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAI,CAACV,mBAAkB,GAAI;QAAE,GAAGU;MAAM,CAAC;MACvC,IAAI,CAACX,QAAO,GAAI,IAAI;IACtB,CAAC;IACD,MAAMY,UAAUA,CAAC;MAAElB,SAAS;MAAEmB;IAAa,CAAC,EAAE;MAC5C,IAAI,IAAI,CAACZ,mBAAkB,IAAK,IAAI,CAACA,mBAAmB,CAACa,EAAE,EAAE;QAC3D;QACA,IAAI;UACF,MAAMT,QAAO,GAAI,MAAMf,KAAK,CAACyB,GAAG,CAAC,kBAAkBF,YAAY,CAACC,EAAE,EAAE,EAAED,YAAY,CAAC;UACnF,IAAIR,QAAQ,CAACW,MAAK,KAAM,GAAG,EAAE;YAC3B,IAAI,CAACb,kBAAkB,CAAC;YACxB,IAAI,CAACc,SAAS,CAAC,CAAC;UAClB,O