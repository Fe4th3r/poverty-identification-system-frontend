{"ast":null,"code":"import EmergencyForm from './EmergencyForm.vue';\nimport axios from 'axios';\nexport default {\n  name: 'EmergencyList',\n  components: {\n    EmergencyForm\n  },\n  props: {\n    studentId: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {\n      emergencies: [],\n      showForm: false,\n      currentEmergency: null\n    };\n  },\n  mounted() {\n    this.fetchEmergencies();\n  },\n  methods: {\n    async fetchEmergencies() {\n      try {\n        const response = await axios.get(`/emergency/${this.studentId}`);\n        this.emergencies = response.data;\n      } catch (error) {\n        console.error('获取突发状况信息失败:', error);\n      }\n    },\n    showAddForm() {\n      this.currentEmergency = null;\n      this.showForm = true;\n    },\n    showEditForm(emergency) {\n      this.currentEmergency = {\n        ...emergency\n      };\n      this.showForm = true;\n    },\n    async handleSave({\n      studentId,\n      emergency\n    }) {\n      if (this.currentEmergency && this.currentEmergency.id) {\n        // 编辑\n        try {\n          const response = await axios.put(`/emergency/${emergency.id}`, emergency);\n          if (response.status === 200) {\n            this.fetchEmergencies();\n            this.resetForm();\n          } else {\n            alert(response.data.message);\n          }\n        } catch (error) {\n          console.error('更新突发状况信息失败:', error);\n          alert('更新失败');\n        }\n      } else {\n        // 添加\n        try {\n          const response = await axios.post(`/emergency/${studentId}`, emergency);\n          if (response.status === 200) {\n            this.fetchEmergencies();\n            this.resetForm();\n          } else {\n            alert(response.data.message);\n          }\n        } catch (error) {\n          console.error('添加突发状况信息失败:', error);\n          alert('添加失败');\n        }\n      }\n    },\n    async deleteEmergency(id) {\n      if (confirm('确定要删除该突发状况信息吗？')) {\n        try {\n          await axios.delete(`/emergency/${id}`);\n          this.fetchEmergencies();\n        } catch (error) {\n          console.error('删除突发状况信息失败:', error);\n        }\n      }\n    },\n    resetForm() {\n      this.showForm = false;\n      this.currentEmergency = null;\n    },\n    formatDate(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return date.toISOString().split('T')[0];\n    }\n  }\n};","map":{"version":3,"names":["EmergencyForm","axios","name","components","props","studentId","type","Number","required","data","emergencies","showForm","currentEmergency","mounted","fetchEmergencies","methods","response","get","error","console","showAddForm","showEditForm","emergency","handleSave","id","put","status","resetForm","alert","message","post","deleteEmergency","confirm","delete","formatDate","dateString","date","Date","toISOString","split"],"sources":["C:\\Users\\Administrator\\poverty-identification-system-frontend\\src\\components\\emergency\\EmergencyList.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h4>突发状况信息</h4>\r\n      <button v-if=\"!showForm\" @click=\"showAddForm\">添加突发状况信息</button>\r\n  \r\n      <table v-if=\"emergencies.length > 0\">\r\n        <thead>\r\n          <tr>\r\n            <th>ID</th>\r\n            <th>突发状况描述</th>\r\n            <th>发生日期</th>\r\n            <th>操作</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"emergency in emergencies\" :key=\"emergency.id\">\r\n            <td>{{ emergency.id }}</td>\r\n            <td>{{ emergency.description }}</td>\r\n            <td>{{ formatDate(emergency.occurrenceDate) }}</td>\r\n            <td>\r\n              <button @click=\"showEditForm(emergency)\">编辑</button>\r\n              <button @click=\"deleteEmergency(emergency.id)\">删除</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <p v-else-if=\"!showForm\">暂无突发状况信息</p>\r\n  \r\n      <div v-if=\"showForm\">\r\n        <EmergencyForm :studentId=\"studentId\" :emergency=\"currentEmergency\" @save=\"handleSave\" @cancel=\"resetForm\" />\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import EmergencyForm from './EmergencyForm.vue';\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    name: 'EmergencyList',\r\n    components: {\r\n      EmergencyForm,\r\n    },\r\n    props: {\r\n      studentId: {\r\n        type: Number,\r\n        required: true,\r\n      },\r\n    },\r\n    data() {\r\n      return {\r\n        emergencies: [],\r\n        showForm: false,\r\n        currentEmergency: null,\r\n      };\r\n    },\r\n    mounted() {\r\n      this.fetchEmergencies();\r\n    },\r\n    methods: {\r\n      async fetchEmergencies() {\r\n        try {\r\n          const response = await axios.get(`/emergency/${this.studentId}`);\r\n          this.emergencies = response.data;\r\n        } catch (error) {\r\n          console.error('获取突发状况信息失败:', error);\r\n        }\r\n      },\r\n      showAddForm() {\r\n        this.currentEmergency = null;\r\n        this.showForm = true;\r\n      },\r\n      showEditForm(emergency) {\r\n        this.currentEmergency = { ...emergency };\r\n        this.showForm = true;\r\n      },\r\n      async handleSave({ studentId, emergency }) {\r\n        if (this.currentEmergency && this.currentEmergency.id) {\r\n          // 编辑\r\n          try {\r\n            const response = await axios.put(`/emergency/${emergency.id}`, emergency);\r\n            if (response.status === 200) {\r\n              this.fetchEmergencies();\r\n              this.resetForm();\r\n            } else {\r\n              alert(response.data.message);\r\n            }\r\n          } catch (error) {\r\n            console.error('更新突发状况信息失败:', error);\r\n            alert('更新失败');\r\n          }\r\n        } else {\r\n          // 添加\r\n          try {\r\n            const response = await axios.post(`/emergency/${studentId}`, emergency);\r\n            if (response.status === 200) {\r\n              this.fetchEmergencies();\r\n              this.resetForm();\r\n            } else {\r\n              alert(response.data.message);\r\n            }\r\n          } catch (error) {\r\n            console.error('添加突发状况信息失败:', error);\r\n            alert('添加失败');\r\n          }\r\n        }\r\n      },\r\n      async deleteEmergency(id) {\r\n        if (confirm('确定要删除该突发状况信息吗？')) {\r\n          try {\r\n            await axios.delete(`/emergency/${id}`);\r\n            this.fetchEmergencies();\r\n          } catch (error) {\r\n            console.error('删除突发状况信息失败:', error);\r\n          }\r\n        }\r\n      },\r\n      resetForm() {\r\n        this.showForm = false;\r\n        this.currentEmergency = null;\r\n      },\r\n      formatDate(dateString) {\r\n        if (!dateString) return '';\r\n        const date = new Date(dateString);\r\n        return date.toISOString().split('T')[0];\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* 可以根据需要添加样式 */\r\n  </style>"],"mappings":"AAmCE,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,KAAK;MACfC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,cAAc,IAAI,CAACZ,SAAS,EAAE,CAAC;QAChE,IAAI,CAACK,WAAU,GAAIM,QAAQ,CAACP,IAAI;MAClC,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,aAAa,EAAEA,KAAK,CAAC;MACrC;IACF,CAAC;IACDE,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACR,gBAAe,GAAI,IAAI;MAC5B,IAAI,CAACD,QAAO,GAAI,IAAI;IACtB,CAAC;IACDU,YAAYA,CAACC,SAAS,EAAE;MACtB,IAAI,CAACV,gBAAe,GAAI;QAAE,GAAGU;MAAU,CAAC;MACxC,IAAI,CAACX,QAAO,GAAI,IAAI;IACtB,CAAC;IACD,MAAMY,UAA